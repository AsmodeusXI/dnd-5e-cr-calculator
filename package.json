{
  "name": "dnd-5e-cr-calculator",
  "version": "1.0.0",
  "description": "A calculator for D&D 5e's Challenge Rating statistic.",
  "main": "dist/calculateChallengeRating.js",
  "scripts": {
    "test": "coffeetape --stderr ./src/**/*Spec.coffee | faucet",
    "test:watch": "watch 'npm run test' src",
    "precompile": "rm -rf dist/ && mkdir dist && mkdir dist/config",
    "compile:coffee": "coffee --compile --output dist/ src/",
    "compile:config": "cp ./src/config/challengeThresholds.json ./dist/config/challengeThresholds.json",
    "compile": "npm run compile:coffee && npm run compile:config"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AsmodeusXI/dnd-5e-cr-calculator.git"
  },
  "author": "Sam Lawton <sam.h.lawton@gmail.com> (http://www.realmshaper.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AsmodeusXI/dnd-5e-cr-calculator/issues"
  },
  "homepage": "https://github.com/AsmodeusXI/dnd-5e-cr-calculator#readme",
  "dependencies": {
    "lodash": "^4.15.0"
  },
  "devDependencies": {
    "coffee-script": "^1.10.0",
    "coffeetape": "^1.0.1",
    "faucet": "0.0.1",
    "tape": "^4.6.0",
    "watch": "^0.19.2"
  }
}
